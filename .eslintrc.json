{
	"env": {
		"browser": true,
    "es2020": true,
    "jest": true
	},
	"extends": [
    "airbnb",
		"airbnb/hooks",
		"plugin:react/recommended"
	],
	"globals": {
		"__webpack_hash__": "readonly",
    "__webpack_devmode__": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
    "react/jsx-one-expression-per-line": 0,
    "object-curly-newline": 0,
    "max-len": ["error", { "code": 160 }]
	}
}